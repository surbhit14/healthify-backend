        <div class="section glass w-100">
            <div class="p-5">
                <h1 class="h1 fw-bold" text-align: center;>Scan successful</h1>
                <br>
                <br>
                <div class=" w-100">
                    <button class="btn shadow btn-warning fw-bold p-3 w-100" onclick="return close();">
                        You can close this window
                    </button>
                </div>
                <hr>
            </div>
        </div>
    </section>

<script>
    function close() {
        let new_window = open(location, '_self');
		new_window.close();
		return false;
        }
</script>



